{
  "scripts": {
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "fix:prettier": "prettier --write \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "lint:eslint": "eslint .",
    "fix:eslint": "eslint --fix .",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "server:dev": "node ./server",
    "server:prod": "cross-env NODE_ENV=production node ./server",
    "dev:node": "npm run server:dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite build && vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.0.0",
    "cross-env": "^7.0.3",
    "prettier": "3.0.1",
    "vite": "^4.4.9",
    "vite-plugin-ssr": "0.4.135"
  }
}
