{
  "imports": {
    "#root/*": "./*.ts"
  },
  "scripts": {
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "fix:prettier": "prettier --write \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "lint:eslint": "eslint .",
    "fix:eslint": "eslint --fix .",
    "dev": "npm run server:dev",
    "prod": "npm run build && npm run server:prod",
    "prod:static": "npm run build && npm run server:static",
    "build": "vite build",
    "server:dev": "ts-node ./server",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server",
    "server:static": "serve dist/client/ --listen 3000"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^14.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-ssr": "0.4.135"
  },
  "devDependencies": {
    "prettier": "3.0.1"
  }
}
