{
  "scripts": {
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "fix:prettier": "prettier --write \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "lint:eslint": "eslint .",
    "fix:eslint": "eslint --fix .",
    "dev": "vite",
    "build": "vite build",
    "start": "npm run build && npm run serve",
    "serve": "concurrently \"npm run serve:cdn \" \"npm run serve:ssr\"",
    "serve:ssr": "node ./server/ssr",
    "serve:cdn": "node ./server/cdn"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.0.3",
    "vite-plugin-ssr": "0.4.135"
  },
  "devDependencies": {
    "prettier": "3.0.1"
  },
  "type": "module"
}
