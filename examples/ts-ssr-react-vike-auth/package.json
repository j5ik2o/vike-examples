{
  "type": "module",
  "scripts": {
    "build": "tsc -p . && vite build",
    "clean": "rimraf ./dist",
    "dev": "npm run server:dev",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "server:dev": "node ./src/server",
    "server:prod": "cross-env NODE_ENV=production node ./src/server"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vike": "^0.4.142",
    "vike-react": "^0.3.0",
    "vite": "^4.4.9"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/express": "4.17.17",
    "@types/node": "^20.4.10",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-react": "^4.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "rimraf": "^5.0.1",
    "typescript": "5.1.x"
  }
}
