{
  "scripts": {
    "build": "vite build",
    "clean": "rimraf ./dist",
    "dev": "vite",
    "dev:node": "npm run server:dev",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,json}\"",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check \"**/*.{js,ts,jsx,tsx,json}\"",
    "preview": "vite build && vite preview",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "server:dev": "node ./src/server",
    "server:prod": "cross-env NODE_ENV=production node ./src/server"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "sirv": "^2.0.3",
    "vite": "^4.4.9",
    "vite-plugin-ssr": "0.4.135"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "rimraf": "^5.0.1"
  }
}
