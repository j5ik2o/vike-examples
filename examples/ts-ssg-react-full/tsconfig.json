{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "types": [
      "node",
      "vite/client"
    ]
  },
  "ts-node": {
    "compilerOptions": {
      "moduleResolution": "Node16"
    },
    "esm": true,
    "transpileOnly": true
  }
}
