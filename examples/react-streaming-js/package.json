{
  "scripts": {
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "fix:prettier": "prettier --write \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "lint:eslint": "eslint .",
    "fix:eslint": "eslint --fix .",
    "dev": "npm run server:dev",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build",
    "server:dev": "node ./server",
    "server:prod": "cross-env NODE_ENV=production node ./server"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.0.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-streaming": "^0.3.14",
    "sirv": "^2.0.3",
    "vite": "^4.4.9",
    "vite-plugin-ssr": "0.4.135"
  },
  "devDependencies": {
    "prettier": "3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3"
  }
}
