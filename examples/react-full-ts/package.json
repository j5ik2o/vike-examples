{
  "scripts": {
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --check \"**/*.{js,ts,jsx,tsx,json}\"",
    "fix:prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,json}\"",
    "lint:eslint": "eslint .",
    "fix:eslint": "eslint --fix .",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "server:dev": "node --loader ts-node/esm ./src/server/index.ts",
    "server:prod": "cross-env NODE_ENV=production node --loader ts-node/esm ./src/server/index.ts",
    "dev:node": "npm run server:dev",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite build && vite preview"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "@mdx-js/mdx": "^2.1.1",
    "@mdx-js/react": "^2.1.1",
    "@mdx-js/rollup": "^2.1.1",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "cross-fetch": "^3.1.5",
    "express": "^4.18.1",
    "node-fetch": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-streaming": "^0.3.9"
  },
  "devDependencies": {
    "typescript": "^4.8.4",
    "vite": "^4.4.9",
    "vite-plugin-ssr": "0.4.135",
    "@vitejs/plugin-react": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.1",
    "prettier": "3.0.1"
  },
  "type": "module"
}
