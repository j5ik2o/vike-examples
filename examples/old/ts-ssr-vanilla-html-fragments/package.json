{
  "type": "module",
  "scripts": {
    "build": "vite build",
    "clean": "rimraf ./dist",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check \"**/*.{vue,js,ts,jsx,tsx,json}\"",
    "dev": "npm run server:dev",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "server:dev": "node --loader ts-node/esm ./src/server/index.ts",
    "server:prod": "cross-env NODE_ENV=production node --loader ts-node/esm ./src/server/index.ts"
  },
  "dependencies": {
    "compression": "1.7.4",
    "express": "^4.17.1",
    "sirv": "^2.0.3",
    "vite": "^4.4.9",
    "vite-plugin-ssr": "0.4.135"
  },
  "devDependencies": {
    "@types/compression": "^1.7.1",
    "@types/express": "4.17.17",
    "@types/node": "20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "prettier": "3.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^5.1.6"
  }
}
